\begin{frame}[t,label=frameA] {Complete Venn Diagram}
\begin{tikzpicture}
\node [draw,
    circle,
    minimum size =3cm,
    label={135:$A$}] (A) at (0,0){};
 
% Set B
\node [draw,
    circle,
    minimum size =3cm,
    label={45:$B$}] (B) at (1.8,0){};
 
% Set intersection label
\node at (0.9,0) {$A\cap B$};
 
\end{tikzpicture}
\end{frame}

